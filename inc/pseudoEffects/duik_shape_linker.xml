<!--


This control is valid for use in the PresetEffects.xml file,
however, keep in mind that some control properties are only
available when applying through the Pseudo Effect Maker and
will not have any effect when imported through the
PresetEffects.xml file. These are the exceptions for your control:

>	point_x point_y and (if using a Point3D) point_z properties will have no effect


-->

<Effect matchname="Pseudo/DUIK shape linker" name="Bone">
	<Point name="$$$/AE/Preset/Vertex=Vertex" default_x="0" default_y="0" point_x="0" point_y="0" />
	<Group name="$$$/AE/Preset/Tangents=Tangents" >
		<Point name="$$$/AE/Preset/Intangent=In tangent" default_x="0" default_y="0" point_x="0" point_y="0" />
		<Point name="$$$/AE/Preset/Outtangent=Out tangent" default_x="0" default_y="0" point_x="0" point_y="0" />
	</Group>
	<Group name="$$$/AE/Preset/ControlLayers=Control Layers" >
		<Layer name="$$$/AE/Preset/Vertex=Vertex" default_self="false" />
		<Layer name="$$$/AE/Preset/InTangent=In Tangent" default_self="false" />
		<Layer name="$$$/AE/Preset/OutTangent=Out Tangent" default_self="false" />
	</Group>
</Effect>
