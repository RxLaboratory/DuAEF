<Effect matchname="Pseudo/DUIK 2d camera" name="$$$/AE/Preset/DUIK2dcamera=2D Camera">
	<Slider name="$$$/AE/Preset/HorizontalPan=Horizontal Pan" default="0" slider_min="-2000" slider_max="2000" valid_min="-32000" valid_max="32000" precision="1" />
	<Slider name="$$$/AE/Preset/VerticalPan=Vertical Pan" default="0" slider_min="-2000" slider_max="2000" valid_min="-32000" valid_max="32000" precision="1" />
	<Slider name="$$$/AE/Preset/Zoom=Zoom" default="0" slider_min="-100" slider_max="100" valid_min="-100000" valid_max="100000" precision="1" DISPLAY_PERCENT="true" />
	<Slider name="$$$/AE/Preset/TruckIn/Out=Truck In/Out" default="0" slider_min="-100" slider_max="100" valid_min="-100000" valid_max="100000" precision="1" DISPLAY_PERCENT="true" />
	<Angle name="$$$/AE/Preset/Tilt=Tilt" default="0" />
	<Group name="$$$/AE/Preset/Movethelayertotruck=Move the layer to truck" dim="true" />
	<Group name="$$$/AE/Preset/(animateitsposition)=(animate its position)" dim="true" />
	<Group name="$$$/AE/Preset/AdvancedBehaviour=Advanced Behaviour" >
		<Popup name="$$$/AE/Preset/Type=Type" hold="true" CANNOT_TIME_VARY="true" default="1" popup_string="Fixed|Hand|Shoulder|Tripod|Steady" />
		<Group name="$$$/AE/Preset/GeneralMotion=General Motion" >
			<Slider name="$$$/AE/Preset/Amplitude=Amplitude" default="5" slider_min="0" slider_max="100" valid_min="0" valid_max="1000" precision="1" DISPLAY_PERCENT="true" />
			<Slider name="$$$/AE/Preset/LoopDuration=Loop Duration" default="0.25" slider_min="0" slider_max="30" valid_min="0" valid_max="10000" precision="2" />
		</Group>
		<Group name="$$$/AE/Preset/Shake=Shake" >
			<Slider name="$$$/AE/Preset/Amplitude=Amplitude" default="0" slider_min="0" slider_max="100" valid_min="0" valid_max="1000" precision="1" DISPLAY_PERCENT="true" />
			<Slider name="$$$/AE/Preset/Frequency=Frequency" default="15" slider_min="-10" slider_max="10" valid_min="-100" valid_max="100" precision="2" />
		</Group>
		<Group name="$$$/AE/Preset/Advanced=Advanced" >
			<Slider name="$$$/AE/Preset/RandomSeed=Random Seed" default="0" slider_min="0" slider_max="10000" valid_min="0" valid_max="10000" precision="0" />
		</Group>
	</Group>
</Effect>