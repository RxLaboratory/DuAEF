		<Color name="$$$/AE/Preset/Color=Color" default_red="236" default_green="24" default_blue="24" />
		<Slider name="$$$/AE/Preset/Size=Size" default="100" slider_min="0" slider_max="1000" valid_min="0" valid_max="10000" precision="0" DISPLAY_PERCENT="true" />
		<Slider name="$$$/AE/Preset/Opacity=Opacity" default="100" slider_min="0" slider_max="100" valid_min="0" valid_max="100" precision="0" DISPLAY_PERCENT="true" />
	</Group>
	<Group name="$$$/AE/Preset/Slider=Slider" >
